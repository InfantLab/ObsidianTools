{
    "name": "obsidian-tools",
    "version": "1.0.0",
    "description": "A collection of tools for working with Obsidian.md vaults - file and property organization utilities",
    "main": "src/index.js",
    "type": "module",
    "scripts": {
        "start": "node src/index.js",
        "dev": "node --watch src/index.js",
        "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
        "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
        "lint": "eslint src/",
        "lint:fix": "eslint src/ --fix"
    },
    "keywords": [
        "obsidian",
        "markdown",
        "file-organization",
        "vault-tools",
        "note-taking"
    ],
    "author": "Caspar",
    "license": "MIT",
    "dependencies": {
        "commander": "^12.1.0",
        "chalk": "^5.3.0",
        "inquirer": "^10.2.2",
        "fs-extra": "^11.2.0",
        "gray-matter": "^4.0.3",
        "glob": "^11.0.0"
    },
    "devDependencies": {
        "jest": "^30.0.0",
        "@eslint/js": "^9.15.0",
        "eslint": "^9.15.0",
        "eslint-config-prettier": "^9.1.0",
        "prettier": "^3.3.3"
    },
    "engines": {
        "node": ">=18.0.0"
    },
    "overrides": {
        "glob": "^11.0.0",
        "inflight": "npm:@isaacs/inflight@^1.0.6",
        "test-exclude": "^7.0.1"
    }
}